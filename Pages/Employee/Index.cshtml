@page
@model RoomBooking.Pages.Employee.IndexModel

@{
    ViewData["Title"] = "Index";
}

<section class="main">
    <div class="head-text">
        <h2>New Employee Registration</h2>
        <img src="data:image/jpeg;base64," class="img-fluid" alt="Responsive image">
    </div>

</section>
<!--for creating new record forEmployees-->
<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Staff[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Staff[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Staff[0].Contact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Staff[0].Designation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Staff)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Contact)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Designation)
                </td>
                <td>
                    <a asp-page="./Edit" asp-route-id="@item.id">Edit</a> |
                    <a asp-page="./Details" asp-route-id="@item.id">Details</a> |
                    <a asp-page="./Delete" asp-route-id="@item.id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
